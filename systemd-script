CONFIG_FILE=/etc/filename.conf
echo "Reading sistest-rotate config file $CONFIG_FILE..."

start () {		
	cfg=`head -n 1 $CONFIG_FILE`
	case $pos in
		a)
      #do a
      ;;
    b)
      #do b
      ;;
    *)
      echo "Wrong configuration file."			
	esac
}

stop () {
}

restart () {
}

force-reload () {
}

status () {
}

case $1 in
	start)
		start
		;;
	stop)
		stop
		;;
	restart)
		restart
		;;
	force-reload)
		force-reload
		;;
	status)
		status
		;;
	*)
		echo "Usage $0: {start|stop|status|restart|force-reload}
esac
exit 0
